#!/bin/bash

# Disable/enable waveform dumping
OMSP_NODUMP=0
export OMSP_NODUMP

OMSP_SIMULATOR=iverilog
export OMSP_SIMULATOR

__SANCUS_SIM=1
export __SANCUS_SIM

DMA_SIM=1
export DMA_SIM

rm -rf ./cov_work
rm -rf ./log/*.log
mkdir  ./log

stdbuf -oL ../bin/msp430sim sancus/sm_dma_peripheral                  | tee ./log/sancus-sm_dma_peripheral.log

# Report regression results
../bin/parse_results
exit $?

